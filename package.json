{
  "name": "@timkurvers/wine-log-explorer",
  "version": "0.5.5",
  "description": "Explorer for Wine logs",
  "author": "Tim Kurvers",
  "repository": "github:timkurvers/wine-log-explorer",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "wine",
    "winehq",
    "crossover"
  ],
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:clean": "rm -rf dist/*",
    "build:publish": "git subtree push --prefix dist origin gh-pages",
    "lint": "eslint .",
    "parse:dev": "./bin/parse.ts",
    "start:dev": "vite",
    "start:preview": "vite preview",
    "style:fix": "prettier . --write",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mantine/core": "^7.11.2",
    "@mantine/dropzone": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@tabler/icons-react": "^3.11.0",
    "@types/react-window": "^1.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "typescript": "^5.5.3",
    "use-async-effect": "^2.2.7",
    "vite-node": "^2.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.0.4",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "prettier": "3.3.2",
    "typescript-eslint": "^8.0.0-alpha.54",
    "vite": "^5.4.8",
    "vitest": "^2.0.2"
  },
  "engines": {
    "node": ">=20.12.x"
  }
}
